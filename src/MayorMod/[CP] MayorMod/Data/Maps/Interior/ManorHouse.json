{
    "Changes": [
        {
            "LogName": "Loading {{ModId}}_ManorHouse into assets",
            "Action": "Load",
            "Target": "Maps/{{ModId}}_ManorHouse",
            "FromFile": "Assets/Maps/Interior/ManorHouse.tmx"
        },
        {
            "Action": "EditData",
            "Target": "Data/Locations",
            "TargetField": [ "ManorHouse", "CreateOnLoad" ],
            "Entries": {
                "MapPath": "Maps/{{ModId}}_ManorHouse"
            },
            "When": {
                "HasFlag: hostPlayer | contains={{ModId}}_ElectedAsMayor": true,
                "HasFlag: hostPlayer | contains={{ModId}}_WonMayorElection": false
            }
        },
        {
            "LogName": "Loading strings for CouncilMeeting",
            "Action": "EditData",
            "Target": "Strings/UI",
            "Entries": {
                "{{ModId}}_HoldCouncilMeeting": "{{i18n:{{ModId}}_UIMenu.HoldCouncilMeeting}}",
                "{{ModId}}_AgendaQuestion": "{{i18n:{{ModId}}_UIMenu.AgendaQuestion}}",
                "{{ModId}}_MeetingPlanned": "{{i18n:{{ModId}}_UIMenu.MeetingPlanned}}",
                "{{ModId}}_AlreadyPlanned": "{{i18n:{{ModId}}_UIMenu.AlreadyPlanned}}",
                "{{ModId}}_NoNewMeetings": "{{i18n:{{ModId}}_UIMenu.NoNewMeetings}}",
                "{{ModId}}_MeetingIntro": "{{i18n:{{ModId}}_UIMenu.MeetingIntro}}",
                "{{ModId}}_MeetingSaloonHours": "{{i18n:{{ModId}}_UIMenu.MeetingSaloonHours}}",
                "{{ModId}}_MeetingTownSecurity": "{{i18n:{{ModId}}_UIMenu.MeetingTownSecurity}}",
                "{{ModId}}_MeetingTownCleanup": "{{i18n:{{ModId}}_UIMenu.MeetingTownCleanup}}"
            }
        }
    ]
}